[core]
  excludesfile = ~/.gitignore_global
  editor = vim
  quotepath = false
  pager = /usr/local/var/homebrew/linked/git/share/git-core/contrib/diff-highlight/diff-highlight | less
[alias]
  aa = add .
  st = status
  ci = commit -v
  co = checkout
  br = branch
  ds = diff --staged
  d  = diff
  ri = rebase -i
  rc = rebase --continue
  rs = rebase --skip
  ra = rebase --abort
  ap = add -p
  sp = stash --patch
  vomit = commit
[user]
  name = Szymon Fiedler
  email = szymon.fiedler@gmail.com
  signingkey = 9BB0E676
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = always
[merge]
  tool = opendiff
[github]
  user = fidel
[help]
  autocorrect = 1
[push]
  default = current
[filter "media"]
  clean = git-media-clean %f
  smudge = git-media-smudge %f
[filter "hawser"]
  clean = git hawser clean %f
  smudge = git hawser smudge %f
  required = true
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[pull]
  rebase = true
[diff]
  word-diff = color
  indentHeuristic = true
  noprefix = true
[commit]
  gpgsign = true
